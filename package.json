{
  "name": "@apparts/api",
  "version": "6.0.0",
  "description": "A unified API package for @apparts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "vitest --globals --exclude './build/**'",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "prepublishOnly": "npm run ci && npm run build",
    "clean": "rimraf coverage build tmp",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true npm run test",
    "ci:eslint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git@ph-uhl.com:Apparts/apparts-frontend-api.git"
  },
  "keywords": [
    "apparts",
    "api",
    "frontend",
    "network"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.21.4",
    "ramda": "^0.28.0"
  },
  "peerDependencies": {
    "@apparts/types": "^10.0.7"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/prep": "^3.3.0",
    "@apparts/types": "^10.0.7",
    "@eslint/js": "^9.34.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/node": "^20.x.x",
    "@types/ramda": "^0.28.14",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/eslint-plugin": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^2.5.1",
    "rimraf": "^6.0.1",
    "supertest": "^5.0.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "testEnvironment": "node",
  "volta": {
    "node": "22.14.0",
    "npm": "10.9.2"
  }
}
