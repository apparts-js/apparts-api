{
  "name": "@apparts/api",
  "version": "3.2.0",
  "description": "A unified API package for @apparts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "testCoverage": "jest --coverage",
    "testOne": "jest ",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "prepublishOnly": "npm run ci && npm run build",
    "clean": "rimraf coverage build tmp",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true jest test",
    "ci:eslint": "eslint . --ext .js,.jsx,.ts,.tsx -c .eslintrc.js"
  },
  "repository": {
    "type": "git",
    "url": "git@ph-uhl.com:Apparts/apparts-frontend-api.git"
  },
  "keywords": [
    "apparts",
    "api",
    "frontend",
    "network"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "dependencies": {
    "@apparts/types": "^6.15.2",
    "axios": "^0.21.4",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.0.3",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/ramda": "^0.28.14",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.5.1",
    "supertest": "^5.0.0",
    "ts-jest": "^27.1.3",
    "tsutils": "^3.21.0",
    "typescript": "^4.5.5"
  },
  "testEnvironment": "node"
}
